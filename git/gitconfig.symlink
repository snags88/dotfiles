[user]
	name = Seiji Naganuma
	email = snaganumas@gmail.com
[alias]
  co = checkout
[push]
	default = simple
[mergetool]
  keepBackup = false
[difftool]
  prompt = false
[core]
  excludesfile = ~/.gitignore
	editor = vim
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
